q = 21888242871839275222246405745257275088696311157297823662689037894645226208583
F1 = GF(q)
C1 = EllipticCurve(F1,[0,3])
cofactor1 = F1(1)
g1 = C1(1,2)
order = C1.order()
identity1 = C1(0, 1, 0)
assert g1*order == identity1

F2 = GF(q^2,"i",modulus=x^2 + 1)
TwistB = 3*F2("9+i")^(-1)

C2 = EllipticCurve(F2,[0,TwistB])
cofactor_2 = F1(21888242871839275222246405745257275088844257914179612981679871602714643921549)
g2 = C2(F2("11559732032986387107991004021392285783925812861821192530917403151452391805634*i + 10857046999023057135944570762232829481370756359578518086990519993285655852781"),
    F2("4082367875863433681332203403145435568316851327593401208105741076214120093531*i + 8495653923123431417604973247489272438418190587263600148770280649306958101930"))
identity2 = C2(0, 1, 0)
assert g2 * order == identity2
